  "description": "ThinkJS 2.x Socket.io adapter, thats implement uWebsockets",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/im-kulikov/think-socket-uws.git"
  },
