    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "2.8.0",
    "istanbul": "0.4.0",
    "mocha": "1.20.1",
    "muk": "0.3.1",
    "source-map": "0.5.3",
